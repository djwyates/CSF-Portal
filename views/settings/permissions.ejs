<%- include("../partials/header") %>

<a href="/settings">Back</a>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Student ID</th>
      <th>Access Level</th>
    </tr>
  </thead>
  <tbody>
    <% members.forEach(function(member) { %>
      <tr>
        <td><%- member.name %></td>
        <td><%- member._id %></td>
        <td><%- member.accessLevel %><a href="/members/<%- member._id %>">Details</a></td>
      </tr>
    <% }); %>
  </tbody>
</table>

<%- include("../partials/footer") %>
